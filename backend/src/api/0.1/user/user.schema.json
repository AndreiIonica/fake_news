{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "User",
  "description": "The user object schema.No required properties as we dont create users here.",
  "default": {},
  "required": [],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database id"
    },
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "The name of the user.Must be unique",
      "examples": ["Trashable Team"]
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The hashed password of the user."
    },
    "role": {
      "$id": "#/properties/role",
      "type": "string",
      "title": "The role of the user.",
      "examples": ["admin", "mod", "normal"]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The date the trashcan_type was created"
    },
    "update_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the trashcan_type was last updated"
    },
    "deleted_at": {
      "$id": "#/properties/deleted_at",
      "type": "string",
      "title": "The date the trashcan_type was deleted"
    }
  }
}
